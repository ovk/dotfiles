alias vi="nvim"
alias vim="nvim"

alias ll='ls -lh --group-directories-first'
alias la='ls -alh --group-directories-first'

alias chown='chown --preserve-root'
alias chmod='chmod --preserve-root'
alias chgrp='chgrp --preserve-root'

alias cp='cp -i'
alias mv='mv -i'
alias rm='rm -I'

set -x GPG_TTY (tty)

